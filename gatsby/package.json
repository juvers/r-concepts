{
  "name": "tishman-gatsby-monorepo",
  "description": "The Monorepo for all things Tishman+Gatsby",
  "scripts": {
    "preinstall": "node ./scripts/netlify-preinstall.js",
    "deduplicate": "yarn-deduplicate && yarn",
    "build": "node ./scripts/tishman build",
    "build-crm": "node ./scripts/build-crm.js",
    "build-parallel": "node ./scripts/tishman build-parallel",
    "clean": "node ./scripts/tishman clean",
    "develop": "node ./scripts/tishman develop",
    "serve": "node ./scripts/tishman serve",
    "lint": "node ./scripts/tishman lint",
    "fix": "node ./scripts/tishman fix",
    "typecheck": "node ./scripts/tishman typecheck",
    "test": "node ./scripts/tishman test",
    "watch": "node ./scripts/tishman watch",
    "generate-sanity-schema": "node ./scripts/generate-sanity-schema"
  },
  "bin": {
    "tishman": "./scripts/tishman.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hzdg/tishman"
  },
  "bugs": {
    "url": "https://github.com/hzdg/tishman/issues"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/"
  },
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=12.16.2",
    "yarn": ">=1.22.10"
  },
  "workspaces": [
